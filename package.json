{
  "name": "@superexpert-ai/rag",
  "version": "0.1.0-alpha.7",
  "description": "Terminal tool to enable Retrieval-Augmented Generation (RAG) with Superexpert AI.",
  "keywords": [
    "superexpert-ai",
    "RAG"
  ],
  "main": "./dist/index.js",
  "bin": {
    "rag": "./dist/index.js"
  },
  "files": [
    "dist",
    "prisma-client",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Superexpert/superexpert-ai-rag"
  },
  "license": "MIT",
  "publishConfig": {
    "access": "public",
    "tag": "alpha"
  },
  "author": "Stephen Walther",
  "scripts": {
    "clean": "rimraf dist",
    "generate": "prisma generate",
    "compile": "tsc -p tsconfig.build.json",
    "build": "npm run clean && npm run generate && npm run compile",
    "postinstall": "node ./scripts/postinstall.js",
    "prepublishOnly": "npm run build",
    "bump:alpha": "npm version prerelease --preid=alpha && npm publish --tag alpha"
  },
  "dependencies": {
    "@prisma/client": "^6.5.0",
    "prisma": "^6.5.0",
    "@types/yargs": "^17.0.33",
    "cli-progress": "^3.12.0",
    "fast-glob": "^3.3.3",
    "js-tiktoken": "^1.0.19",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@types/cli-progress": "^3.11.6",
    "@types/node": "^22.14.1",
    "rimraf": "^5.0.0",
    "typescript": "^5.8.2"
  }
}
